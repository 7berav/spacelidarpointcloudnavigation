<robot name="simple_cylinder">
  <link name="cylinder_link">
    <gravity>false</gravity>
    <visual>
      <origin xyz = "0 0 0" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="3" length="5"/>
      </geometry>
      <material name="red">
        <color rgba="0.8 0.1 0.1 1.0"/>
      </material>
    </visual>
    <collision>
      <origin xyz = "0 0 0" rpy = "0 0 0"/>
      <geometry>
        <cylinder radius="3" length="2"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia
        ixx="0.05" ixy="0.0" ixz="0.0"
        iyy="0.05" iyz="0.0"
        izz="0.02"/>
    </inertial>
  </link>
  <gazebo>
    <static>true</static>
  </gazebo>
</robot>

